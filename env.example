# StructureScout Trading Bot - Environment Variables
# IMPORTANT: Copy this file to .env and fill in your actual credentials.
# The .env file is in .gitignore and will NOT be committed to version control.

# MetaTrader5 Configuration
MT5_LOGIN=your_account_number_here
MT5_PASSWORD=your_password_here
MT5_SERVER=your_broker_server_here
MT5_PATH=/path/to/mt5/installation

# OpenAI API Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Optional Settings
ENVIRONMENT=development
DEBUG_MODE=false
LOG_LEVEL=INFO
